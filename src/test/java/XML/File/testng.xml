<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">




<suite name="CrossBrowserTestSuite"
       parallel="tests" thread-count="3">

    <test name="AliexpressChrome">
        <parameter name="browserTipi" value="chrome"></parameter>

        <classes>
            <class name="Runners.Cross_BrowserTesting"></class>
        </classes>

    </test>

    <test name="AliexpressFirefox">
        <parameter name="browserTipi" value="firefox"></parameter>

        <classes>
            <class name="Runners.Cross_BrowserTesting"></class>
        </classes>

    </test>

    <test name="AliexpressEdge">
        <parameter name="browserTipi" value="edge"></parameter>

        <classes>
            <class name="Runners.Cross_BrowserTesting"></class>
        </classes>

    </test>



</suite>
